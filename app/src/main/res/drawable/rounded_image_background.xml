<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#FFF"/> <!-- 배경색 -->
            <corners android:radius="50dp"/> <!-- 이 값을 조정하여 원하는 둥근 정도를 얻을 수 있습니다. -->
        </shape>
    </item>
    <item android:right="1dp" android:top="1dp" android:left="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="#FFF"/> <!-- 여기에 원하는 배경색을 지정하세요. -->
            <corners android:radius="50dp"/>
        </shape>
    </item>
</layer-list>
